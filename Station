package ike1;
import java.util.HashSet;
import java.util.Set;

public class Station {
	
	    String name;
	    Set<String> lines;

	    public Station(String name) {
	        this.name = name;
	        this.lines = new HashSet<>();
	    }

	    public void addLine(String line) {
	        lines.add(line);
	    }

	    @Override
	    public String toString() {
	        return name + " " + lines;
	    }
	}

